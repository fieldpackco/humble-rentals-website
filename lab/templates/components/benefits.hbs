<section class="benefits" id="benefits">
    <h2>{{title}}</h2>
    <div class="benefits-grid">
        {{#each items}}
        <div class="benefit-card animate-in">
            <div class="benefit-icon">{{icon}}</div>
            <h3>{{title}}</h3>
            <p>{{description}}</p>
        </div>
        {{/each}}
    </div>
</section>
