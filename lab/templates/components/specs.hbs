<section class="section" id="specs">
    <h2 class="section-title">{{title}}</h2>
    <div class="grid grid-4">
        {{#each items}}
        <div class="card">
            <div class="spec-value">{{value}}</div>
            <div class="card-description">{{label}}</div>
        </div>
        {{/each}}
    </div>
</section>
