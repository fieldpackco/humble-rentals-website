<section class="specs" id="specs">
    <h2>{{title}}</h2>
    <div class="specs-grid">
        {{#each items}}
        <div class="spec-item">
            <div class="spec-value">{{value}}</div>
            <div class="spec-label">{{label}}</div>
        </div>
        {{/each}}
    </div>
</section>
