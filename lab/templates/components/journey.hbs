<section class="journey" id="journey">
    <h2>{{title}}</h2>
    <div class="journey-comparison">
        <div class="journey-item">
            <h3>{{comparison.without.title}}</h3>
            <p>{{comparison.without.description}}</p>
        </div>
        <div class="journey-item">
            <h3>{{comparison.with.title}}</h3>
            <p>{{comparison.with.description}}</p>
        </div>
    </div>
</section>
