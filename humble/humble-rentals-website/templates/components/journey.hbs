<section class="section bg-white" id="journey">
    <h2 class="section-title">{{title}}</h2>
    <div class="grid grid-2">
        <div class="card">
            <h3 class="card-title">{{comparison.without.title}}</h3>
            <p class="card-description">{{comparison.without.description}}</p>
        </div>
        <div class="card">
            <h3 class="card-title">{{comparison.with.title}}</h3>
            <p class="card-description">{{comparison.with.description}}</p>
        </div>
    </div>
</section>
